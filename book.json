{
    "gitbook": "3.2.3",
    "title": "Kingreatwill's Blog",
    "author": "Kingreatwill",
    "links": {
        "sidebar": {
            "学习笔记 ": "https://github.com/openjw/open",
            "GitHub": "https://github.com/BuiltCloud"
        }
    },
    "plugins": [
        "-sharing",
        "-lunr", "-search", "search-pro",   
        "anchor-navigation-ex",
        "built@git+https://github.com/BuiltCloud/gitbook-plugin-built.git"
    ],
    "pluginsConfig": {
        "img-popup": true,
        "favicon": "./img/logo/favicon.ico",
        "logo": {
            "url": "../img/logo/logo.png",
            "style": "background: none; max-height: 30px; min-height: 30px",
            "href": "./"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "code": {
            "line": true,
            "copyButton": true
        },
        "github-buttons": {
            "buttons": [{
                "user": "openjw",
                "repo": "open",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },       
        "page-footer": {
            "description": "modified at",
            "signature": "Kingreatwill",
            "wisdom": "More than a coder, more than a designer",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; BuiltCloud all right reserved, powered by <a href='https://github.com/BuiltCloud' target='_blank'>kingreatwill</a>",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": true,
            "baseUri": "http://localhost:4000/",
            "isShowIssues": true,
            "repo": "openjw/open",
            "issueNum": "8",
            "token": "",
            "style": "normal"
        },
        "anchor-navigation-ex": {
            "showLevel": true
        }
    }
}